{
  "name": "rdx-model",
  "version": "0.0.2",
  "description": "redux model",
  "main": "dist/es5/src/index.js",
  "types": "./types/index.d.ts",
  "module": "dist/es6/src/index.js",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "dev": "webpack-dev-server --open --mode development",
    "test": "./node_modules/jest/bin/jest.js --color",
    "build": "npm run clean && npm run build:es5 -- --sourceMap && npm run build:es6 -- --sourceMap && npm run build:var",
    "build:var": "webpack --mode production",
    "build:es5": "tsc -p . --outDir dist/es5",
    "build:es6": "tsc -p . -m es6 --outDir dist/es6",
    "build:prod": "npm run clean && npm run build:es5 && npm run build:es6",
    "watch:es5": "npm run build:es5 -- -w",
    "watch:es6": "npm run build:es6 -- -w",
    "clean": "rm -rf ./dist",
    "prepush": "npm run lint && npm run build:es5 -q",
    "prepack": "npm run lint && npm run build:prod -q",
    "start": "node scripts/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juicecube/rxModel.git"
  },
  "keywords": [
    "redux"
  ],
  "author": "wangfukai",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^11.13.5",
    "@types/uuid-js": "^0.7.2",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^2.0.1",
    "colors": "^1.3.3",
    "generator-tsc-library": "^0.1.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "redux": "^4.0.4",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "tslint": "^5.19.0",
    "typescript": "^3.4.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "yosay": "^2.0.2"
  },
  "dependencies": {
    "@types/redux-actions": "^2.6.1",
    "redux-actions": "^2.6.5"
  }
}
